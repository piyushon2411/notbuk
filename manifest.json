{
  "manifest_version": 3,
  "name": "NotBuk",
  "version": "1.0",
  "description": "A simple note editor in your new tab.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ],
  "icons": {
    "16": "images/favicon.ico",
    "32": "images/favicon.ico",
    "48": "images/favicon.ico",
    "64": "images/favicon.ico",
    "128": "images/favicon.ico"
  }
}
